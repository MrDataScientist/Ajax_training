{
  "parser": "babel-eslint",
    "extends": [
      "standard",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "babel",
      "react"
    ],
    "rules": {
      "key-spacing"          : 0,
      "jsx-quotes"           : [2, "prefer-single"],
      "max-len"              : [2, 120, 2],
      "object-curly-spacing" : [2, "always"]
    }
}

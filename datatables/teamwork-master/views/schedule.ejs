<div class="container" style="background-color:rgb(255, 255, 255);width:1000px;height:60px;border:1px solid silver; margin-top:20px;">
    <div class="container" style="width:180px;height:40px;margin-top:8px;margin-bottom:5px;margin-left:420px;margin-right:400px;" >
        <a href="/projectpage/<%= projectid%>/addschedule"><img src="/image/PlusMember.jpg" alt="添加日程"/>添加日程</a>
    </div>
</div>
<%schedule.forEach(function(sch){%>
<div class="container" style="width:1000px;height:230px;margin-top:20px;border:1px solid silver;">
    <div class="container" style="width:1000px;height:60px;float:left;margin-left:0px;padding-left:0px;">
        <div class="col-xs-6" style="border:0px solid silver;height:50px;width:70px;padding-left:5px;padding-bottom:20px;"><p class="text-left" style="font-size:24px;margin-top:5px;">日程</p></div>
        <div class="col-xs-10" style="border:0px solid silver;height:50px;width:900px;"><p class="text-muted" style="margin-top:10px;margin-left:0px;padding-left:0px;font-size:20px;"><%=sch.sch_name%></p></div>
    </div>
    <div class="container" style="width:1000px;height:160px;">
        <div class="container" style="width:900px;height:140px;border:1px solid silver;margin-top:50px;margin-left:20px;margin-right:30px;">
            <div class="container" style="margin-top:10px;margin-bottom:10px;margin-left:30px;margin-right:30px;border:0px solid silver;height:50px;width:750px;">
                <div class="col-xs-1" style="border: 0px solid silver; height: 45px;"><p class="text-primary" style="font-size:40px;"> T </p></div>
                <div class="col-xs-8" style="margin-left: 0px; border: 0px solid silver;"><p class="text-muted" style="font-size:25px;margin-top:10px;"><%=sch.sch_start%> ~  <%=sch.sch_end%></p></div>
            </div>
            <div class="container" style="margin-top:10px;margin-bottom:10px;margin-left:30px;margin-right:30px;border:0px solid silver;height:50px;width:750px;">
                <div class="col-xs-1" style="border: 0px solid silver; margin-left: 0px; height: 45px;"><p class="text-primary" style="font-size:40px;"> P </p></div>
                <div class="col-xs-4" style="margin-left: 0px; border: 0px solid silver;height:45px;"><p class="text-muted" style="font-size:25px;margin-top:10px;"><%= sch.sch_place%></p></div>
                <div class="col-xs-1" style="border:0px solid silver;height:45px;width:90px;"><p class="text-muted" style="font-size: 18px; margin-top: 10px;">参与者</p></div>
                <div class="col-xs-5" style="border:0px solid silver;height:45px;width:310px;">
                    <% sch.avatars.forEach(function(ava){%>
                    <img class="img3" src="/image/avatar/<%= ava%>" alt="用户1的头像"/>
                    <!--<img class="img3" src="/image/avatar/default.jpg" alt="用户1的头像"/>-->
                    <!--<img class="img3" src="/image/avatar/default.jpg" alt="用户1的头像"/>-->
                    <!--<img class="img3" src="/image/avatar/default.jpg" alt="用户1的头像"/>-->
                    <!---->
                    <%})%>
                </div>
            </div>
        </div>
    </div>
</div>
<% })%>
</body>


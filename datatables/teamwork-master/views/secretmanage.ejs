            <a  href="../mainpage"><li class="list-group-item"><span style="color:gray;font-weight:bold">返回主页</span></a>
            </li>
        </ul>
    </div>

    <div class="col-xs-6">

        <div class="container" style="background-color:white; height: 400px; width:600px;margin-left:50px;">
            <form class="form-horizontal" role="form" style="margin-top:20px" method="post">
                <div class="form-group"style="margin-top:100px">
                    <label for="oldpassword" class="col-sm-3 control-label" style="color:gray">
                        旧密码
                    </label>
                    <div class="col-sm-7">
                        <input type="password" class="form-control" id="firstname" placeholder="" name="old_pass" required autofocus>
                    </div>
                </div>
                <div class="form-group">
                    <label for="newpassword" class="col-sm-3 control-label" style="color:gray">新密码</label>
                    <div class="col-sm-7">
                        <input type="password" class="form-control" id="firstname" placeholder="" name="new_pass1" required autofocus>
                    </div>
                </div>
                <div class="form-group">
                    <label for="newpassword" class="col-sm-3 control-label" style="color:gray">确认新密码</label>
                    <div class="col-sm-7">
                        <input type="password" class="form-control" id="lastname" placeholder="" name="new_pass2" required autofocus>
                    </div>
                </div>
                <%if(error){%>
                <div class="col-sm-5" >
                <div class="alert alert-danger warning" role="alert">
                    <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                    <span class="sr-only">Error:</span>
                   <%= error%>
                </div>
                </div>
                <%}%>
                <div class="form-group" style="color: CornflowerBlue;margin-left:150px">
                    <div class="col-sm-offset-2 col-sm-10">
                        <button type="submit" class="btn btn-primary btn-lg" style="color:white;width:100px">保存</button>
                    </div>
                </div>
            </form>
        </div>

    </div>
